<h1>Listing tipo_pokemons</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Pokemon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_pokemons.each do |tipo_pokemon| %>
      <tr>
        <td><%= tipo_pokemon.tipo %></td>
        <td><%= tipo_pokemon.pokemon %></td>
        <td><%= link_to 'Show', tipo_pokemon %></td>
        <td><%= link_to 'Edit', edit_tipo_pokemon_path(tipo_pokemon) %></td>
        <td><%= link_to 'Destroy', tipo_pokemon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo pokemon', new_tipo_pokemon_path %>
